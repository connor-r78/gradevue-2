{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "protocol", "value": "http" }],
      "destination": "https://:host/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        }
      ]
    }
  ]
}
